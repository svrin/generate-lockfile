{
  "name": "generate-lockfile",
  "version": "0.0.1",
  "description": "CLI to generate lockfile from a workspace lockfile and package.json",
  "main": "index.js",
  "author": "Chris Pavlicek <chrisp@freetrade.io>",
  "license": "MIT",
  "scripts": {
    "watch": "yarn nodemon --watch \"src/**\" --ext \"ts\" --exec \"ts-node src/index.ts\" -p './package.json' -l './yarn.lock'"
  },
  "bin": {
    "generate-lockfile": "./bin/index.js"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "chalk": "^4.1.0",
    "commander-plus": "^0.0.6",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.2"
  }
}
